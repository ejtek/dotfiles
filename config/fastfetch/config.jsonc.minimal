      "key": "󰅶",
      "format": "{2}h {3}m {4}s",
    },
    {
      "type": "packages",
      "key": "󰏓",
      "format": "{1}",
    },
    {
      "type": "processes",
      "key": "",
      "format": "{1}",
    },
    {
      "type": "memory",
      "key": "󰍛",
      "format": "{1}",
    },
    {
      "type": "localip",
      "key": "󰩠",
      "format": "{1}",
    },
  ],
}