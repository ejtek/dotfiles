  "logo": {
    "source": "arch_small",
    "padding": {
      "left": 1,
      "right": 2,
    },
  },
  "display": {
    "separator": "  ",
    "color": "bright_green",
  },
  "modules": [
    {
      "type": "os",
      "key": "",
